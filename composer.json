{
    "name": "mohamedahmed01/laravel-correlation",
    "description": "Laravel Correlation ID Middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Ahmed",
            "email": "mohamedabdelmenem01@email.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mohamedahmed01\\LaravelCorrelation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mohamedahmed01\\LaravelCorrelation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mohamedahmed01\\LaravelCorrelation\\CorrelationServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}